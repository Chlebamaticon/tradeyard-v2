{
  "name": "trade-contract",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/contracts/trade-contract/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/contracts/trade-contract",
        "main": "apps/contracts/trade-contract/src/index.ts",
        "tsConfig": "apps/contracts/trade-contract/tsconfig.lib.json",
        "assets": ["apps/contracts/trade-contract/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../../coverage/apps/contracts/trade-contract"
      }
    }
  },
  "tags": []
}
